Процедура СоздатьПоказанияНаСервере()
	СтруктураДокументов = Новый Структура;
	СтруктураДокументовНепредоставлений = Новый Структура;
	Запрос = Новый Запрос;
	ЗапросСреднее = Новый Запрос;
	Запрос.Текст = ""ВЫБРАТЬ
	               |	ркПоказанияСчетчиков.Счетчик КАК Счетчик,
	               |	ркПоказанияСчетчиков.ЛицевойСчет КАК ЛицевойСчет,
	               |	МАКСИМУМ(ркПоказанияСчетчиков.Период) КАК Период
	               |ПОМЕСТИТЬ втДатаПоказания
	               |ИЗ
	               |	РегистрНакопления.ркПоказанияСчетчиков КАК ркПоказанияСчетчиков
	               |		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ркСостояниеСчетчиков.СрезПоследних(&Месяц, ) КАК ркСостояниеСчетчиковСрезПоследних
	               |		ПО ркПоказанияСчетчиков.Счетчик = ркСостояниеСчетчиковСрезПоследних.Счетчик
	               |ГДЕ
	               |	ркПоказанияСчетчиков.Период < &Месяц
	               |	И ркСостояниеСчетчиковСрезПоследних.Включен
	               |	И (ркПоказанияСчетчиков.ЛицевойСчет = &ЛицевойСчет
	               |			ИЛИ &ЛицевойСчет = ЗНАЧЕНИЕ(Справочник.рклицевыесчета.пустаяссылка))
	               |	И ркСостояниеСчетчиковСрезПоследних.Счетчик.Владелец ССЫЛКА Справочник.ркПомещения
	               |
	               |СГРУППИРОВАТЬ ПО
	               |	ркПоказанияСчетчиков.Счетчик,
	               |	ркПоказанияСчетчиков.ЛицевойСчет
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	ВложенныйЗапрос.Счетчик,
	               |	ВложенныйЗапрос.ЛицевойСчет,
	               |	ВложенныйЗапрос.Период,
	               |	МАКСИМУМ(ркПоказанияСчетчиков.НачальноеКоличество + ркПоказанияСчетчиков.Количество) КАК КонечныеПоследние
	               |ИЗ
	               |	втДатаПоказания КАК ВложенныйЗапрос
	               |		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.ркПоказанияСчетчиков КАК ркПоказанияСчетчиков
	               |		ПО ВложенныйЗапрос.Счетчик = ркПоказанияСчетчиков.Счетчик
	               |			И ВложенныйЗапрос.ЛицевойСчет = ркПоказанияСчетчиков.ЛицевойСчет
	               |			И ВложенныйЗапрос.Период = ркПоказанияСчетчиков.Период
	               |
	               |СГРУППИРОВАТЬ ПО
	               |	ВложенныйЗапрос.Счетчик,
	               |	ВложенныйЗапрос.ЛицевойСчет,
	               |	ВложенныйЗапрос.Период
	               |
	               |ОБЪЕДИНИТЬ ВСЕ
	               |
	               |ВЫБРАТЬ РАЗЛИЧНЫЕ
	               |	ркСостояниеСчетчиковСрезПоследних.Счетчик,
	               |	ркЛицевыеСчета.Ссылка,
	               |	ДАТАВРЕМЯ(2014, 10, 1),
	               |	рцПоказанияСчетчиковСрезПоследних.НачальныеПоказания + рцПоказанияСчетчиковСрезПоследних.Количество
	               |ИЗ
	               |	РегистрСведений.ркСостояниеСчетчиков.СрезПоследних(&Месяц, ) КАК ркСостояниеСчетчиковСрезПоследних
	               |		ЛЕВОЕ СОЕДИНЕНИЕ втДатаПоказания КАК втДатаПоказания
	               |		ПО ркСостояниеСчетчиковСрезПоследних.Счетчик = втДатаПоказания.Счетчик
	               |		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.рцПоказанияСчетчиков.СрезПоследних(&Месяц, ) КАК рцПоказанияСчетчиковСрезПоследних
	               |		ПО ркСостояниеСчетчиковСрезПоследних.Счетчик = рцПоказанияСчетчиковСрезПоследних.Счетчик
	               |		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ркЛицевыеСчета КАК ркЛицевыеСчета
	               |		ПО ркСостояниеСчетчиковСрезПоследних.Счетчик.Владелец = ркЛицевыеСчета.Помещение
	               |ГДЕ
	               |	втДатаПоказания.Счетчик ЕСТЬ NULL 
	               |	И ркСостояниеСчетчиковСрезПоследних.Включен
	               |	И (ркЛицевыеСчета.Ссылка = &ЛицевойСчет
	               |			ИЛИ &ЛицевойСчет = ЗНАЧЕНИЕ(Справочник.рклицевыесчета.пустаяссылка))"";
	Запрос.УстановитьПараметр(""Месяц"", Месяц);		
	Запрос.УстановитьПараметр(""ЛицевойСчет"", ЛС);
	ТЗ = Запрос.Выполнить().Выгрузить();
	КопияТЗ = ТЗ.Скопировать();
	КопияТЗ.Свернуть(""Период"");
	КопияТЗ.Сортировать(""Период"");
	МинимальныйМесяц = КопияТЗ[0].Период;
	Пока МинимальныйМесяц < Месяц Цикл
	
		//Док = Документы.ркПоказанияСчетчиков.СоздатьДокумент();
		//Док.Дата = МинимальныйМесяц;
		//Док.ПоказанияГрупповыхСчетчиков = Ложь;
